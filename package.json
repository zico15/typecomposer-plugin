{
  "name": "typecompose-plugin",
  "type": "module",
  "version": "0.1.2",
  "packageManager": "pnpm@8.11.0",
  "description": "TypeCompose plugin for Vite",
  "author": "zico15 <zequielzico@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/zico15/typecompose-plugin#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/zico15/typecompose-plugin.git"
  },
  "bugs": "https://github.com/zico15/typecompose-plugin/issues",
  "keywords": [
    "vite",
    "vite-plugin",
    "framework"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./client": {
      "types": "./client.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist",
    "pwa",
    "public"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "copy": "cp -r dist /Users/ezequiel/Documents/TypeCompose/vite-project/node_modules/typecompose-plugin",
    "build": "esno scripts/build.ts && npm run copy",
    "pub": "npm run build && npm publish --access public --tag latest"
  },
  "peerDependencies": {
    "vite": "^3.1.0 || ^4.0.0 || ^5.0.0",
    "ts-morph": "^20.0.0"
  },
  "devDependencies": {
    "ts-morph": "^20.0.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.0"
  }
}
